    "file=open('input1.txt','r')\n",
    "n=file.read()\n",
    "s=int(n)\n",
    "stp=0\n",
    "while s>0:\n",
    "    a=max(list(n))\n",
    "    s-=int(a)\n",
    "    stp+=1\n",
    "file2=open('output1.txt','w')\n",
    "file2.write(str(stp))"
   ]
